<!DOCTYPE html>
<html>
  <head>
    <title>PhotoAlbum</title>
    <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track' => true %>
    <%= javascript_include_tag 'application', 'data-turbolinks-track' => true %>
    <%= csrf_meta_tags %>
  </head>
  <body>
    <%= render 'header' %>
    <div class="container">
      <%#= render 'sub_row' %>
      <div class="row">
        <%= yield %>
      </div>
      <%#= render 'footer' %>
    </div>
  </body>
</html>
